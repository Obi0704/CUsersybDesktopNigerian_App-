rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    // TEMPORARY: Allow everything for testing (30 days)
    // Remove this block and uncomment below when ready for production
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2026, 1, 5);
    }
    
  }
}